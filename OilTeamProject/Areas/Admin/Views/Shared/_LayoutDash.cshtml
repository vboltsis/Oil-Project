<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.5.0/css/all.css" integrity="sha384-B4dIYHKNBt8Bc12p+WXckhzcICo0wtJAoU8YZTY5qE0Id1GSseTk6S+L3BlXeVIU" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u"
          crossorigin="anonymous">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
    <script src="https://cloud.tinymce.com/stable/tinymce.min.js"></script>
    @Styles.Render("~/Content/toastr.css")
    <script>tinymce.init({ selector: 'textarea' });</script>
    @Styles.Render("~/Dashboard/dashboard.css")
    @Scripts.Render("~/bundles/modernizr")

    <style type="text/css">

        #queue {
            background-color: #FFF;
            border: 1px solid #E5E5E5;
            border-radius: 5px;
            height: 178px;
            margin-bottom: 5px;
            overflow: auto;
            padding: 10px;
            width: 300px;
        }
    </style>

</head>
<body>

    <!--navbar with blue area-->
    <nav>
        <!--.container-fluid #topNav-->
        <div class="container-fluid" id="topNav">
            <!--.row-fluid with user option and breadcrump-->
            <div class="row">
                <div class="col-md-2" style="background-image:url(../../../../Image/logoBC5companyDash.jpg); background-size:contain;"></div>




                <div class="col-md-7" id="oldcontent">

                </div>
                <div class="col-md-3">
                    <ul class="pull-right list-unstyled list-inline  account-border sidebar-link">
                        <li>
                            <img width="30" alt="" src="../Image/account.png">
                            &nbsp; @Html.Partial("_LoginPartialDash")
                        </li>
                    </ul>
                </div>
            </div>
            <!--end .row-fluid with user option and breadcrump-->
        </div>
        <!--end .container-fluid #topNav-->
    </nav>
    <!-- end navbar with blue area-->
    <!--.container-fluid #dashboard-->
    <div class="container-fluid" id="dashboard">
        <!-- ROW sidebar + main-content -->
        <div class="row">
            <!-- col-md-2 start (Sidebar) -->
            @Html.Partial("_DashNavbar")
            <!-- col-md-2 end (Sidebar) -->
            <!-- COL-MD-10 Breadcrump + List-Group start-->
            <div class="col-md-10">

                <!-- ROW:list-group start -->
                <div class="row">
                    <div class="col-md-12">

                        @RenderBody()

                    </div>
                </div>
            </div> <!-- COL-MD-10 Breadcrump + List-Group end-->
        </div><!-- ROW sidebar + main-content end -->
    </div><!-- end .container-fluid #dashboard-->



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    @Scripts.Render("~/Scripts/toastr.js")

    @Scripts.Render("~/Scripts/app.js")
</body>
</html>
